            {% for history in histories %}
				<div id="{{ history.id }}" class="full-history full-history_disable ">
					<div class="full-history-content">
						<img class="history__logo js__logo logo-first" src="{{ history.image }}">
						<h5 id="logo-title" class="logo-title full__title">{{ history.title }}</h5>
						<a href="#" class="whatch__full js-history-back"><div class="arrow__back"></div><span class="whatch__full_txt">Вернуться ко всем историям</span></a>
					</div>
					<div class="history-feed full-feed">
						<p id="feed" class="feed feed-full">{{ history.text|safe }}
						</p>
					</div>
				</div>
